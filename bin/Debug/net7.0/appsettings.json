{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ServiceName": "OpenTelemetry.Template",

  "OpenTelemetry": {
    "Exporters": {
      "Otlp": {
        "Endpoint": "http://localhost:4317"
      }
    },
    "EnableTracing": true,
    "EnableMetrics": true,
    "EnableLogging": true
  }
}
